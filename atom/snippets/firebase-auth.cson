".text.html":
  "Firebase Auth":
    "prefix": "firebase-auth"
    "body": """
      <firebase-auth
      	id="${1:auth}"
      	user="{{${2:user}}}"
      	status-known="{{${3:statusKnown}}}"
      	location="https://${4:<YOUR-FIREBASE-APP>}.firebaseio.com"
      	provider="{{${5:provider}}}"
      	on-error="${6:_authOnError}">
      </firebase-auth>$0
    """
